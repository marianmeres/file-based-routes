{
	"openapi": "3.0.0",
	"info": { "title": "Test server" },
	"servers": [{ "url": "http://0.0.0.0:9999" }],
	"paths": {
		"/hello": {
			"post": {
				"summary": "Foo hello summary",
				"requestBody": {
					"required": true,
					"content": {
						"application/json": {
							"schema": {
								"type": "object",
								"properties": { "foo": { "type": "string" } }
							}
						}
					}
				},
				"responses": {
					"200": {
						"content": {
							"application/json": {
								"schema": {
									"type": "object",
									"properties": {
										"id": { "type": "integer" },
										"foo": { "type": "string" }
									}
								}
							}
						}
					}
				}
			}
		},
		"/hello/{name}": {
			"get": {
				"summary": "Hey ho",
				"parameters": [
					{
						"in": "path",
						"name": "name",
						"required": true,
						"schema": { "type": "string", "pattern": "^\\d+$" }
					}
				]
			}
		}
	},
	"components": { "schemas": {} }
}
